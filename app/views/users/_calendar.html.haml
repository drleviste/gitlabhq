#cal-heatmap
  :javascript 
    window.timestamps = #{@timestamps};
    var cal = new CalHeatMap();
      cal.init({
        itemName: ["commit"],
        data: window.timestamps,
        start: new Date(#{@time_copy.year-1}, #{@time_copy.month}),
        domainLabelFormat: "%b",
        id : "cal-heatmap",
        domain : "month",
        subDomain : "day",
        range : 12, 
        tooltip: true,
        domainDynamicDimension: false,
        colLimit: 4,
        label: { position: "top" },
        domainMargin: 1,
        legend: [0,1,2,5],
        legendCellPadding: 3,
        // subDomainTitleFormat: {count} {"commit"} {date}
      });

